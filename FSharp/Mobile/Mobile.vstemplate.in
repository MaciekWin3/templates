<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
  <TemplateData>
    <Name>WebSharper 5 Mobile Application (.NET Framework)</Name>
    <Description>Creates a single-page HTML application which can be packaged in a Cordova mobile app.</Description>
    <Icon>WebSharper.png</Icon>
    <ProjectType>FSharp</ProjectType>
    <ProjectSubType>WebSharper</ProjectSubType>
    <RequiredFrameworkVersion>4.6.1</RequiredFrameworkVersion>
    <SortOrder>1000</SortOrder>
    <TemplateID>91a2cdc9-8ffd-40c6-8b0b-88b48bbf0790</TemplateID>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Mobile</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
    <LanguageTag>FSharp</LanguageTag>
    <PlatformTag>iOS</PlatformTag>
    <PlatformTag>Android</PlatformTag>
    <ProjectTypeTag>Mobile</ProjectTypeTag>
  </TemplateData>
  <TemplateContent>
    <Project File="ProjectTemplate.fsproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true">AssemblyInfo.fs</ProjectItem>
      <ProjectItem ReplaceParameters="true">Client.fs</ProjectItem>
      <ProjectItem ReplaceParameters="true">index.html</ProjectItem>
      <ProjectItem ReplaceParameters="true" OpenInEditor="true">readme.txt</ProjectItem>
      <ProjectItem>wsconfig.json</ProjectItem>
    </Project>
  </TemplateContent>
  <WizardExtension>
    <Assembly>{vsixassembly}</Assembly>
    <FullClassName>WebSharper.Vsix.VSVersionWizard</FullClassName>
  </WizardExtension>
  <WizardExtension>
    <Assembly>NuGet.VisualStudio.Interop, Version=1.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
    <FullClassName>NuGet.VisualStudio.TemplateWizard</FullClassName>
  </WizardExtension>
  <WizardData>
    <packages repository="extension" repositoryId="IntelliFactory.WebSharper.470db229-6bcc-48a4-a41c-fe3c36bb4b2c">
      <package id="System.ValueTuple" targetFramework="net461" />
      <package id="FSharp.Core" targetFramework="net461" />
      <package id="WebSharper" targetFramework="net461" />
      <package id="WebSharper.FSharp" targetFramework="net461" />
      <package id="WebSharper.UI" targetFramework="net461" />
    </packages>
  </WizardData>
</VSTemplate>